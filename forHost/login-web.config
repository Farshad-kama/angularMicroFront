<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <add name="Access-Control-Allow-Origin" value="*" />
                <add name="X-Powered-By" value="ASP.NET" />
            </customHeaders>
        </httpProtocol>
        <staticContent>
            <mimeMap fileExtension=".mjs" mimeType="text/javascript" />
        </staticContent>
    </system.webServer>
</configuration>
